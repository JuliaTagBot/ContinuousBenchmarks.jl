# -*- mode: conf -*-
[server]
http_ip = "0.0.0.0"
http_port = 5900
log_level = "info" # debug|info|warn|error
stop_file = "config/stop-server"
bm_runner = "travis" # travis|self

[turing]
path = "{{{ :project_root }}}/../Turing.jl"
use_remote_branches = true

[github]
# for main repo
app_repo = "TuringLang/TuringBenchmarks"

# for App
app_id = 1000
priv_pem = "config/xxxx.pem"
secret = "the webhook secret"

# for App Owner
user = "username"
email = "user_email"
token = "token"
